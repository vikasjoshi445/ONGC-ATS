# .env
# This file stores environment variables for your application.
# It should NOT be committed to version control (e.g., Git).

# JWT Secret for authentication tokens
# Replace 'your-super-secret-jwt-key' with a strong, unique key for production.
JWT_SECRET=your-super-secret-jwt-key

# Port for the Express.js server
# If not set, the server will default to port 3001 as defined in server/index.js
SERVER_PORT=5001

# Other potential environment variables could include:
DATABASE_URL=sqlite://./server/database/ongc_ats.db
NODE_ENV=development